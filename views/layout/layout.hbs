     <script>
       let val =  document.querySelector(".sidebar").value
       console.log(val)
     
       if(val=='accountdetails'){
        document.getElementById("accountdetails").style.backgroundColor = 'red';
        document.querySelector(".a").style.color = 'white';

       }
       if(val=='dashboard'){
        document.getElementById("dashboard").style.backgroundColor = 'red';
        document.querySelector(".a").style.color = 'white';

       }
       if(val=='orderhistory'){
        document.getElementById("orderhistory").style.backgroundColor = 'red';
        document.querySelector(".a").style.color = 'white';

       }
       if(val=='trackorder'){
        document.getElementById("trackorder").style.backgroundColor = 'red';
        document.querySelector(".a").style.color = 'white';

       }
       if(val=='shoppingcart'){
        document.getElementById("shoppingcart").style.backgroundColor = 'red';
        document.querySelector(".a").style.color = 'white';

       }
       if(val=='wishlist'){
        document.getElementById("wish").style.backgroundColor = 'red'
         document.querySelector(".a").style.color = 'white';


       }
       if(val==='address'){
        document.getElementById("address").style.backgroundColor = 'red';
        document.querySelector(".a").style.color = 'white';

       }
       if(val==='wallet'){
        document.getElementById("wallet").style.backgroundColor = 'red';
        document.querySelector(".a").style.color = 'white';

       }
       if(val==='coupon'){
        document.getElementById("coupon").style.backgroundColor = 'red';
        document.querySelector(".a").style.color = 'white';

       }

      
    </script>



    


